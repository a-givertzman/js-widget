{
  "name": "js-widget",
  "version": "0.0.3",
  "description": "JavaScript framework to build object-oriented fast and lightweight web application, easy to understand, test and maintain",
  "source": "./index.html",
  "directories": {
    "test": "test"
  },
  "alias": {
    "js-widget/**": "/src/lib/$1"
  },
  "scripts": {
    "debug": "rm -rf dist/* && parcel ./index.html",
    "build": "rm -rf dist/* && parcel build ./index.html --dist-dir dist --public-url ./",
    "test-nav": "rm -rf dist/* && parcel ./test/navigator/test_navigator.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-givertzman/js-widget.git"
  },
  "keywords": [
    "framework",
    "web",
    "js",
    "oop",
    "elegant-objects"
  ],
  "author": "anton lobanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-givertzman/js-widget/issues"
  },
  "homepage": "https://github.com/a-givertzman/js-widget#readme",
  "devDependencies": {
    "parcel": "^2.4.0"
  }
}
