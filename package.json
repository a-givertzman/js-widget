{
  "name": "js-widget",
  "version": "0.0.2",
  "description": "JavaScript framework to build object-oriented fast and lightweight web application, easy to understand, test and maintain",
  "main": "index.js",
  "module": "dist/$1.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "debug": "rm -rf dist/* && parcel ./src/index.js",
    "build": "rm -rf dist/* && parcel build ./src/js-widgets/*.js --dist-dir dist --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-givertzman/js-widget.git"
  },
  "keywords": [
    "framework",
    "web",
    "js",
    "oop",
    "elegant-objects"
  ],
  "author": "anton lobanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-givertzman/js-widget/issues"
  },
  "homepage": "https://github.com/a-givertzman/js-widget#readme",
  "devDependencies": {
    "parcel": "^2.4.0"
  }
}
